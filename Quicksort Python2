# --- Quicksort ---
# quick sort algorithm for python


def quicksort(array):
    if len(array) < 2:
        return array
    else:
        pivot = array[0]
        less = [ i for i in array[1:] if i <= pivot]
        greater = [i for i in array[1:] if i > pivot]
        return quicksort(less) + [pivot] + quicksort(greater)

a = [1,3,2,4,7,20,8,45,99,102,5,6]
print quicksort(a)
